
mod TYPE is

  sorts Type TypeError .

endm

mod PRIMITIVE-TYPES is

  pr TYPE .

  op Boolean : -> Type .
  op Integer : -> Type .
  op Decimal : -> Type .
  op String : -> Type .

endm

mod UNDEFINED-TYPE is

  pr TYPE .

  op Undefined : TypeError -> Type .

endm

mod TYPES is
  pr PRIMITIVE-TYPES .
  pr UNDEFINED-TYPE .
endm
